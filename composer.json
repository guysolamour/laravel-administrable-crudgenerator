{
  "name": "guysolamour/laravel-administrable-crudgenerator",
  "description": "A package to generate crud for administrable package. This package can not be used alone.",
  "license": "MIT",
  "keywords": [
    "crud",
    "generator",
    "laravel"
  ],
  "type": "library",
  "authors": [
    {
      "name": "Guy-roland ASSALE",
      "email": "rolandassale@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "symfony/yaml": "^5.0",
    "illuminate/support": "^8.0"
  },
  "require-dev": {
    "orchestra/testbench": "^4.0|^5.0|^6.0",
    "phpunit/phpunit": "^8.4|^9.0"
  },
  "autoload": {
    "psr-4": {
      "Guysolamour\\Administrable\\Crudgenerator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Guysolamour\\Administrable\\Crudgenerator\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpunit": "phpunit"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Guysolamour\\Administrable\\Crudgenerator\\ServiceProvider"
      ]
    }
  },
  "config": {
    "preferred-install": "dist",
    "sort-packages": true,
    "optimize-autoloader": true
  }
}
